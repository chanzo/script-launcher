{
  "scripts": {
    "build:$config": [
      "echo version: $launch_setting_${config}_version",
      "echo ng build -c=$config --deploy-url $launch_setting_${config}_url",
      "echo $launch_time_current"
    ],
    "serve:$config": [
      "echo name: $launch_setting_name",
      "echo version: $launch_setting_${config}_version",
      "echo ng serve -c=$config --deploy-url $launch_setting_${config}_url"
    ],
    "test": [
      "echo '<!-- version: \\$app_version (\\$project-\\$environment)-->' > index.html",
      "#replace --quiet '\\$hallo' 'hoi'         ./test.md",
      "cat index.html",
      "echo Exmaple 1: \\$2",
      "echo Exmaple 2: \\$PATH",
      "echo Exmaple 3: \\${2}",
      "echo Exmaple 4: \\${PATH}",
      "echo 'test \\$app_version'"
    ]
  }
}