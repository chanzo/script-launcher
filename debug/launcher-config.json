{
  "scripts": {
    "build:$config": [
      "echo version: $launch_setting_${config}_version",
      "echo ng build -c=$config --deploy-url $launch_setting_${config}_url",
      "echo $launch_time_current"
    ],
    "serve:$config": [
      "echo name: $launch_setting_name",
      "echo version: $launch_setting_${config}_version",
      "echo ng serve -c=$config --deploy-url $launch_setting_${config}_url"
    ],
    "test": [
      "echo 'arg1' \"arg2\" arg3"
    ]
  }
}
