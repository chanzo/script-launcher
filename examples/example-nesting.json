{
  "scripts": {
    "sequential": {
      "sequential": [
        "echo Sequential 1 && sleep 1",
        "echo Sequential 2 && sleep 1"
      ]
    },
    "concurrent": {
      "concurrent": [
        "sequential",
        "sequential"
      ]
    },
    "nest": {
      "concurrent": [
        "concurrent",
        "concurrent"
      ]
    },
    "env": [
      "data=10",
      "echo Node version: $npm_config_node_version",
      "echo Data: $data $bas"
    ],
    "terraform:$command:$environment": [
      "echo pipeline/$environment",
      "echo AWS_PROFILE=pairbot-$environment",
      "echo terraform $command $1 -var release=$npm_package_version",
      "echo args: '$0' '$1' '$2' '$3' '$4' '$5' '$6'"
    ],
    "build:$environment": [
      "echo Building: $environment $*",
      "echo test",
      "sequential"
    ],
    "deploy:$environment": [
      "build:$environment 11 22",
      "terraform:init:$environment",
      "terraform:apply:$environment"
    ],
    "test:$command": [
      "terraform:$state:acc yy zz",
      "echo args: '$0' '$1' '$2' '$3' '$4' '$5' '$6'"
    ],
    "order:bas1:$value2": "echo Order2: bas1 $value2",
    "order:$value1:$value2": "echo Order1: $value1 $value2",
    "order:bas1:bas2": "echo Order3: bas1 bas2",
    "myFunc:$funcParam1:$funcParam2": [
      "echo Function Parameters: param1=$funcParam1  param2=$funcParam2",
      "echo Function Arguments: argument1=$1  argument2=$2",
      "echo Function All arguments: $*"
    ],
    "build-stuff:$myParam1:$myParam2": [
      "echo Parameters: param1=$myParam1  param2=$myParam2",
      "echo Arguments: argument1=$1  argument2=$2",
      "echo All arguments: $*",
      "echo -------------------------------------------------------",
      "myFunc:$myParam1:funcParam2 funcArg1"
    ]
  }
}