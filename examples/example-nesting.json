{
  "scripts": {
    "sequential": {
      "sequential": [
        "echo Sequential 1 && sleep 1",
        "echo Sequential 2 && sleep 1"
      ]
    },
    "concurrent": {
      "concurrent": [
        "sequential",
        "sequential"
      ]
    },
    "nest": {
      "concurrent": [
        "concurrent",
        "concurrent"
      ]
    },
    "env": [
      "data=10",
      "echo Node version: $npm_config_node_version",
      "echo Data: $data $bas"
    ],
    "terraform20:$command:$environment": [
      "echo pipeline/$environment",
      "echo AWS_PROFILE=pairbot-$environment",
      "echo terraform $command $1 -var release=$npm_package_version",
      "echo args: '$0' '$1' '$2' '$3' '$4' '$5' '$6'"
    ],
    "build:$environment": [
      "echo Building: $environment $*",
      "echo test",
      "sequential"
    ],
    "deploy2:$environment": [
      "build:$environment 11 22",
      "terraform:init:$environment",
      "terraform:apply:$environment"
    ],
    "test:$command": [
      "terraform:$state:acc yy zz",
      "echo args: '$0' '$1' '$2' '$3' '$4' '$5' '$6'"
    ],
    "order:bas1:$value2": "echo Order2: bas1 $value2",
    "order:$value1:$value2": "echo Order1: $value1 $value2",
    "order:bas1:bas2": "echo Order3: bas1 bas2",
    "terraform:$command:$environment": [
      "echo pipeline/$environment",
      "echo AWS_PROFILE=pairbot-$environment",
      "echo terraform $command $*"
    ],
    "terraform:apply:$environment": "terraform:apply:$environment -var release=$npm_package_version",
    "terraform:destroy:$environment": "terraform:destroy:$environment -var release=$npm_package_version",
    "build:$project:$config": "echo ng build $project -c=$config",
    "deploy:$project:$config": [
      "build:$project:$config",
      "echo firebase deploy --public dist/$project -P $project-$config"
    ],
    "deploy:$config": [
      "deploy:uva:$config",
      "deploy:hva:$config"
    ],
    "data1:$a": "echo data1 $a",
    "data2:$b": "echo data2 $b"
  }
}
