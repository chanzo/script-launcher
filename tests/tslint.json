{
  "extends": [
    "../tslint.json"
  ],
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "never"
        },
        "esSpecCompliant": true
      }
    ]
  }
}